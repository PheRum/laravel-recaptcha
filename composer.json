{
    "name": "pherum/laravel-recaptcha",
    "type": "library",
    "description": "google-recaptcha for Laravel 5",
    "keywords": [
        "recaptcha",
        "captcha",
        "laravel",
        "laravel5"
    ],
    "homepage": "https://github.com/pherum/laravel-recaptcha",
    "license": "MIT",
    "authors": [
        {
            "name": "PheRum",
            "email": "pherum@mail.ru"
        }
    ],
    "require": {
        "google/recaptcha": "^1.1",
        "illuminate/support": "^5",
        "illuminate/http": "^5"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.8 | ~5.5"
    },
    "autoload": {
        "psr-4": {
            "PheRum\\Recaptcha\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PheRum\\Recaptcha\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PheRum\\Recaptcha\\RecaptchaServiceProvider"
            ],
            "aliases": {
                "Recaptcha": "PheRum\\Recaptcha\\Facades\\Recaptcha"
            }
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable"
}
